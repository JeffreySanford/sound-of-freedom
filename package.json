{
  "name": "sound-creator",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "nx": "nx",
    "postinstall": "node ./tools/postinstall-assets.js || true",
    "start": "nx serve",
    "start:frontend": "nx serve frontend",
    "start:api": "nx serve api",
    "dev:frontend": "cross-env API_URL=http://127.0.0.1:3000 pnpm -w exec nx serve frontend --port=4200",
    "dev:api": "cross-env PORT=3000 REDIS_URL=redis://127.0.0.1:6379 JEN1_URL=http://127.0.0.1:4001/ MUSCGEN_URL=http://127.0.0.1:4002/ OLLAMA_URL=http://127.0.0.1:11434 pnpm -w exec nx serve api",
    "dev:api:watch": "pnpm -w exec nx build api --watch",
    "dev:api:run": "pnpm -w exec nodemon --watch 'dist/apps/api' --ext js --exec 'node dist/apps/api/main.js'",
    "start:all": "nx run-many --target=serve --projects=frontend,api --parallel",
    "start:all:docker": "node tools/scripts/start-all-docker.js",
    "start:all:docker:down": "docker compose -f docker-compose.yml down -v --remove-orphans",
    "start:all:docker:logs": "docker compose -f docker-compose.yml logs -f",
    "start:all:docker:local": "cross-env FORCE_LOCAL_SERVE=frontend,api node tools/scripts/start-all-docker.js",
    "start:env": "cross-env FORCE_LOCAL_SERVE=frontend,api node tools/scripts/start-all-docker.js --include=frontend,api",
    "start:env:docker": "docker compose -f docker-compose.yml up -d --build",
    "start:env:down": "pnpm start:all:docker:down",
    "start:env:logs": "pnpm start:all:docker:logs",
    "build": "nx build",
    "lint": "nx lint",
    "lint:md": "pnpm exec markdownlint-cli2 \"documetation/**/*.md\" --config .markdownlint.json && pnpm exec remark . --frail",
    "lint:md:fix": "pnpm exec markdownlint-cli2 \"documetation/**/*.md\" --config .markdownlint.json --fix",
    "format:md": "pnpm exec remark . --output . && pnpm exec prettier --write \"documetation/**/*.md\"",
    "test": "nx test",
    "format": "prettier --write \"**/*.{ts,js,json,md,css,scss}\""
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.17",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/cli": "^19.2.17",
    "@angular/compiler-cli": "^19.2.17",
    "@angular/core": "^19.2.17",
    "@nrwl/angular": "^19.8.4",
    "@nrwl/nest": "^19.8.4",
    "@nx/webpack": "22.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.6.0",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "http-server": "^14.1.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli2": "^0.20.0",
    "nx": "22.2.0",
    "prettier": "^2.8.0",
    "remark-cli": "^12.0.1",
    "remark-preset-lint-recommended": "^7.0.0",
    "typescript": "~5.5.3",
    "webpack": "5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "prepare": "husky install",
  "lint-staged": {
    "*.md": [
      "pnpm run format:md",
      "pnpm exec markdownlint-cli2 --fix",
      "pnpm exec remark --quiet --output .",
      "pnpm exec prettier --write",
      "git add"
    ],
    "documetation/**/*.md": [
      "pnpm run format:md",
      "pnpm exec markdownlint-cli2 --fix",
      "pnpm exec remark --quiet --output .",
      "pnpm exec prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/compiler": "^19.2.17",
    "@angular/animations": "^19.2.17",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.17",
    "@angular/forms": "^19.2.17",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.17",
    "@angular/platform-browser-dynamic": "^19.2.17",
    "@angular/router": "^19.2.17",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "11.1.9",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/websockets": "^11.1.9",
    "@ngrx/effects": "^19.2.0",
    "@ngrx/entity": "^19.2.0",
    "@ngrx/store": "^19.2.0",
    "@ngrx/store-devtools": "^19.2.0",
    "ajv": "^8.13.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^7.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "pnpm": {
    "overrides": {
      "micromatch": "4.0.8",
      "esbuild": "0.25.0",
      "http-proxy-middleware": "2.0.9",
      "webpack-dev-server": "5.2.1",
      "tmp": "0.2.4",
      "js-yaml": "4.1.1",
      "koa": "2.16.3"
    }
  }
}
