{
  "name": "sound-creator",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "ollama:start:dev": "bash scripts/docker/start-ollama-only.sh",
    "ollama:check": "bash scripts/docker/check-ollama.sh",
    "poc:send-narrative": "node scripts/poc/send-narrative-via-api.js",
    "nx": "nx",
    "postinstall": "node ./tools/postinstall-assets.js || true",
    "start": "nx serve",
    "start:frontend": "nx serve frontend",
    "start:api": "nx serve api",
    "start:all": "pnpm start:all:docker",
    "start:all:docker": "node tools/scripts/start-all-docker.js",
    "start:all:docker:down": "docker compose -f docker-compose.yml down -v --remove-orphans",
    "start:all:docker:logs": "docker compose -f docker-compose.yml logs -f",
    "start:all:docker:local": "cross-env FORCE_LOCAL_SERVE=frontend,api node tools/scripts/start-all-docker.js",
    "start:env": "cross-env FORCE_LOCAL_SERVE=frontend,api node tools/scripts/start-all-docker.js --include=frontend,api",
    "start:env:docker": "docker compose -f docker-compose.yml up -d --build",
    "start:env:down": "pnpm start:all:docker:down",
    "start:env:logs": "pnpm start:all:docker:logs",
    "test:all": "nx run-many --target=test --projects=frontend,api --parallel",
    "build": "nx build",
    "lint": "nx run-many --target=lint --all",
    "lint:md": "pnpm exec markdownlint-cli2 \"documetation/**/*.md\" --config .markdownlint.json && pnpm exec remark . --frail",
    "lint:md:fix": "pnpm exec markdownlint-cli2 \"documetation/**/*.md\" --config .markdownlint.json --fix",
    "format:md": "pnpm exec remark . --output . && pnpm exec prettier --write \"documetation/**/*.md\"",
    "test": "nx run-many --target=test --all",
    "poc:jen1": "node scripts/poc/generate-jen1.js",
    "test:ollama": "node scripts/ollama/test-ollama-lyrics.js",
    "test:e2e": "node scripts/test/e2e/run-e2e.js",
    "test:e2e:ci": "node scripts/test/e2e/run-e2e.js --ci",
    "test:e2e:ci:debug": "node scripts/test/e2e/run-e2e.js --ci --debug",
    "check:jen1-torch": "node scripts/jen1/check-torch.js",
    "generate:orchestrator-token": "node scripts/auth/generate-orchestrator-token.js",
    "format": "prettier --write \"**/*.{ts,js,json,md,css,scss}\""
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/cli": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@nestjs/testing": "^11.1.9",
    "@nrwl/angular": "^17.0.0",
    "@nrwl/nest": "^17.0.0",
    "@nx/webpack": "^17.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^4.3.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.6.0",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "http-server": "^14.1.0",
    "husky": "^8.0.3",
    "jasmine-core": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.6.2",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.0.3",
    "markdownlint-cli2": "^0.20.0",
    "nx": "^17.0.0",
    "prettier": "^2.8.0",
    "remark-cli": "^12.0.1",
    "remark-preset-lint-recommended": "^7.0.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^6.0.1"
  },
  "prepare": "husky install",
  "lint-staged": {
    "*.md": [
      "pnpm run format:md",
      "pnpm exec markdownlint-cli2 --fix",
      "pnpm exec remark --quiet --output .",
      "pnpm exec prettier --write",
      "git add"
    ],
    "documetation/**/*.md": [
      "pnpm run format:md",
      "pnpm exec markdownlint-cli2 --fix",
      "pnpm exec remark --quiet --output .",
      "pnpm exec prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^19.2.16",
    "@angular/compiler": "17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "11.1.9",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/websockets": "^11.1.9",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/entity": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "@types/express": "^5.0.6",
    "ajv": "^8.13.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "mongoose": "^7.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.8.1",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.0",
    "winston-mongodb": "^5.0.0",
    "zone.js": "~0.14.10"
  }
}
